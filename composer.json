{
    "name": "zazmaster/query-builder-parser-eloquent",
    "homepage": "http://github.com/ZAZmaster/QueryBuilderParserEloquent",
    "description": "Build complex Eloquent & QueryBuilder queries automatically when using jQuery-QueryBuilder",
    "keywords": [ "ajax", "eloquent", "jquery", "laravel", "parser", "query builder", "sql", "mongodb" ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Groeneveld",
            "email": "tim@timg.ws",
            "homepage": "http://timg.ws"
        }
    ],
    "autoload": {
        "psr-4": {
          "zazmaster\\": "src/QueryBuilderParserEloquent/",
          "zazmaster\\test\\": "tests/"
        }

    },
    "minimum-stability": "dev",
    "require": {
        "illuminate/database": "^6.0||4.1 - 6.0"
    },
    "suggest": {
        "jenssegers/mongodb": "Use QueryBuilderParserEloquent with MongoDB"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3|^7.0",
        "mockery/mockery": "^1.2.3|^0.9.4"
    }
}
